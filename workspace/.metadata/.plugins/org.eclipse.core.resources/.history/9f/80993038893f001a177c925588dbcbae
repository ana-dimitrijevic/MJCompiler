program P
{	

	int izraz(int x, int y) {
		return ((x+y) / (x-y)) % 2;
	}
	
	bool bul(char c) {
		if (c=='A' || c=='a') {
			return true;
		}
		else return false;
	}
	
	char karakter(int x, int y) {
	
		if ((x-y>=-10 && x+y<10) || x==y) return 'b';
		else return 'c';
		
	}
	
	void main() int i; bool b; char c; {
	
		
	}
	

}